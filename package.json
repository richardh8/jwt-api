{
  "name": "jwt",
  "version": "1.0.0",
  "description": "RESTful API for managing animals with JWT authentication",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning dist/app.js",
    "build": "tsc",
    "dev": "nodemon --exec node --no-warnings=ExperimentalWarning --loader ts-node/esm src/app.ts",
    "dev:ts": "ts-node --esm src/app.ts",
    "test": "NODE_OPTIONS='--no-warnings=ExperimentalWarning' jest"
  },
  "keywords": [
    "node",
    "typescript",
    "express",
    "jwt",
    "rest-api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2"
  }
}
